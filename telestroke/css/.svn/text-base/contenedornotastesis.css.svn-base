#notasTesis{
    position: relative;
}
.areaTesis{
    margin-top: 10px;
    border: double;
    width: 100%;
}
.encabezadoTesis{
    border-top: double;
    border-bottom: double;
    border-color: grey;
    margin-bottom: 5px;
    padding: 8px;
}
.etiquetasEncabezado{
    padding: 8px;
    margin: 8px;
}
.contenidoIsque{
    clear: both;
}
.etPorcentajeTesis{
    left: 13%;
    position: absolute;
}
.porcentajeTesis{
    width: 3em;
    background-color: #ffffcc;
    border: double;
    float: left;
    left: 13%;
    position: absolute;
}
.nombreArea{
    text-align: center;
    margin:0px auto;
    font: Verdana, Geneva, sans-serif;
    font-size:large
}
.notasTotal{
    float: right;
}
.notasTotal input{
    width: 2.5em;
    font: bold;
    size: 3;
    font-size: large;
}
.notaTotal4{
    color: #900;
}
.notaTotal5{
}
.notaArea4{
	float: right;
	width: 2.5em;
    font: bold;
}
.indicadorAbet{
    width: 3em;
    border-color: transparent;
    font: italic;
}
table th{
    text-align: center;
}
.contenidoIsque table td{
    text-align: center;
    border-color: #600;
    border-width: 1px 1px 0 0;
    border-style: solid;
    margin: 0;
    padding: 4px;
}
.contenidoIsque table th{
    text-align: center;
    border-color: #600;
    border-width: 1px 1px 0 0;
    border-style: solid;
    margin: 0;
    padding: 4px;
    background-color: aliceblue;
}
.contenidoIsque table{
    border-color: #600;
    border-width: 0 0 1px 1px;
    border-style: solid;
    border-spacing: 0;
    border-collapse: collapse;
}
.contenidoIsque textarea{
    width: 90%;
    border-width:0;
    overflow: auto;
}
.subporcentajeTesis{
    width: 3em;
    background-color: #69C;
    border: double;
    float: left;
}
.textMatrizArea{
    font-size: 8pt;
}
.notasDetalle{
}
.notasDetalle input{
    width: 2.5em;
    font: bold;
    size: 3;
}
td.seleccionHover{
    background-color: grey;
}
td.seleccionClic{
    background-color: #C00;
}
#cuadrosPopup{
    width: 100%;
    height: 100%;
    z-index: 9999;
    background-image: url(../img/trans-bg.png);
    background-repeat: repeat;
    position: fixed;
    top: 0;
    left: 0;
}
.nuevosDatos{
    position: absolute;
    top: 50%;
    left: 50%;
    width:350px;
    height:200px;
    margin-top: -100px; /*set to a negative number 1/2 of your height*/
    margin-left: -175px; /*set to a negative number 1/2 of your width*/
    border: 2px solid #2982C6;
    background-color: #FFF;
    text-align: center;
    vertical-align: middle;
    z-index: 10000;
}
