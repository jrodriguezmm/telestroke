.datosEditarInfo{
    position: fixed;
    top: 50%;
    left: 50%;
    width:950px;
    height:550px;
    margin-top: -250px; /*set to a negative number 1/2 of your height*/
    margin-left: -475px; /*set to a negative number 1/2 of your width*/
    border: 2px solid #2982C6;
    background-color: #FFF;
    z-index: 10000;
}
.informacion{
    border: 1px solid;
    width:950px;
    height:500px;
    overflow:scroll;
}
.filasTabla{
    font-family: Arial, Helvetica, sans-serif; font-size: 12px;
}

.encabezadoFilaTabla{
    font-family: Arial, Helvetica, sans-serif; font-size: 12px;
    background-color: grey;
}
.filaHoverTabla{
    background-color: lightgreen;
}
.nuevosDatos{
    position: fixed;
    top: 50%;
    left: 50%;
    width:450px;
    height:230px;
    margin-top: -100px; /*set to a negative number 1/2 of your height*/
    margin-left: -175px; /*set to a negative number 1/2 of your width*/
    border: 2px solid #2982C6;
    background-color: #FFF;
    z-index: 10000;
}
.cuadrosPopup{
    display:none;
    position:fixed;
    _position:fixed; /* hack for internet explorer 6*/
    height:100%;
    width:100%;
    top:0;
    left:0;
    background-image: url(../img/trans-bg.png);
    background-repeat: repeat;
    border:1px solid #cecece;
    z-index:9999;
}